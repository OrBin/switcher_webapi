{
  "name": "switcher_webapi",
  "version": "0.2.4",
  "description": "REST api wrapper for the aioswitcher pypi module running in a Docker image, used to integrate with the Switcher water heater.",
  "keywords": [
    "smarthome",
    "switcher",
    "water heater",
    "docker",
    "rest api"
  ],
  "homepage": "https://switcher-webapi.readthedocs.io/en/stable/",
  "bugs": {
    "url": "https://github.com/TomerFi/switcher_webapi/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Tomer Figenblat",
    "email": "tomer.figenblat@gmail.com",
    "url": "https://github.com/TomerFi"
  },
  "contributors": [],
  "engines": {},
  "files": [
    ".remarkrc"
  ],
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TomerFi/switcher_webapi.git"
  },
  "scripts": {
    "lint-md": "remark --rc-path .remarkrc --frail . .github",
    "lint-yml": "prettier --config .prettierrc.yml --check *.yml **/*.yml",
    "validate-pkg": "pjv --filenmae package.json --spec npm --warnings --recommendations",
    "spell-md-interactive": "mdspell --en-us --ignore-numbers --ignore-acronyms '*.md' '.github/**/*.md'",
    "spell-md-report": "mdspell --en-us --ignore-numbers --ignore-acronyms --report '*.md' '.github/**/*.md'"
  },
  "dependencies": {
    "markdown-spellcheck": "1.3.1",
    "package-json-validator": "0.6.3",
    "prettier": "1.18.2",
    "remark-cli": "6.0.1",
    "remark-frontmatter": "1.3.2",
    "remark-lint": "6.0.4",
    "remark-preset-lint-consistent": "2.0.2",
    "remark-preset-lint-markdown-style-guide": "2.1.2",
    "remark-preset-lint-recommended": "3.0.2"
  },
  "private": true
}
